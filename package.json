{
  "name": "cyberpunk-react",
  "version": "0.0.1",
  "description": "cyberpunk-ui base React",
  "main": "dist/lib/cyberpunk-react.js",
  "types": "dist/lib/cyberpunk-react.d.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.prod.js",
    "test": "cross-env NODE_ENV=test jest --config jest.config.js --runInBand",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyberpunk-ui/cyberpunk-react.git"
  },
  "keywords": [
    "React",
    "react-ui",
    "cuberpunk",
    "ui",
    "ui-library"
  ],
  "author": "Evan Yan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cyberpunk-ui/cyberpunk-react/issues"
  },
  "homepage": "https://github.com/cyberpunk-ui/cyberpunk-react#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@types/jest": "^26.0.10",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-test-renderer": "^16.9.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^26.3.0",
    "conventional-changelog-cli": "^2.1.0",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.2.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.4.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "svg-sprite-loader": "^5.0.0",
    "ts-jest": "^26.2.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
